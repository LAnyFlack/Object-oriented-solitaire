package base;

public class CardBox {
	private int positionx=0;
	private int positiony=0;
	private int length=0;
	private int width=0;
	CardBox(int px,int py){
		positionx=px;
		positiony=py;
	}
	int getX(){
		return positionx;
	}
	int getY(){
		return positiony;
	}
	void setX(int px){
		positionx=px;
	}
	void setY(int py){
		return py;
	}
}